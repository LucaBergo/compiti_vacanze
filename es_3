

#include <iostream>
using namespace std;


int inserimento(){
	while(true){
		
		int numero;
		cout<<"Inserisci un numero ";
		cin>>numero;
		if (numero>=0){	
			return numero;	
		}
		else if (numero<0) {	
			break;
		}	
	}	
}

int main(){
	
	int a,b;
	a=inserimento();
	b=inserimento();
	if (b%a==0){
		
		cout<<b<<" e' multiplo di "<<a;
	}
	else if(b%a!=0){
		cout<<b<<" non e' multiplo di "<<a;
	}
	
	
	
}
