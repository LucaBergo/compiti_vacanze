#include <iostream>
using namespace std;


int inserimento(){
	while(true){
		
		int numero;
		cout<<"Inserisci un numero ";
		cin>>numero;
		if (numero>=0){	
			return numero;	
		}
		else if (numero<0) {	
			break;
		}	
	}	
}

